{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": { "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/db/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/product/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/cached/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/categories",
      "dest": "/api/index.py"
    },
    {
      "src": "/offers",
      "dest": "/api/index.py"
    },
    {
      "src": "/test-offers",
      "dest": "/api/index.py"
    },
    {
      "src": "/graphql",
      "dest": "/api/index.py"
    },
    {
      "src": "/search",
      "dest": "/api/index.py"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "VERCEL_ENV": "production",
    "PYTHONPATH": "."
  },
  "functions": {
    "api/index.py": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
